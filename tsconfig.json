{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": [
      "esnext",
      "esnext.asynciterable",
      "dom"
    ],
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "jsx": "preserve",
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ]
    },
    "allowJs": true,
    "types": [
      "@types/node",
      "@nuxt/vue-app",
      "@nuxtjs/axios",
      "./types"
    ]
  },
  "include": [
    "types/**/*",
    "components/**/*",
    "layouts/**/*",
    "middleware/**/*",
    "pages/**/*",
    "plugins/**/*",
    "server/**/*",
    "store/**/*"
  ]
}
